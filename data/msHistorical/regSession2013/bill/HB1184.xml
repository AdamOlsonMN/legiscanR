<legiscan><status></status><bill><bill_id>475463</bill_id><change_hash>1256210b6b01902ecbef0667ca2e843c</change_hash><session><session_id>1008</session_id><session_name>2013 Regular Session</session_name></session><url>http://legiscan.com/MS/bill/HB1184/2013</url><state_link>http://billstatus.ls.state.ms.us/2013/pdf/history/HB/HB1184.xml</state_link><completed>1</completed><status>6</status><status_date>2013-02-05</status_date><progress><step><date>2013-01-21</date><event>1</event></step><step><date>2013-01-21</date><event>9</event></step><step><date>2013-02-05</date><event>6</event></step></progress><state>MS</state><state_id>24</state_id><bill_number>HB1184</bill_number><bill_type>B</bill_type><body>H</body><body_id>57</body_id><current_body>H</current_body><current_body_id>57</current_body_id><title>Medicaid; reduce hospital assessment if hospital closes inpatient specialty beds or services.</title><description>An Act To Amend Section 43-13-145, Mississippi Code Of 1972, To Provide That The Hospital Assessment For Medicaid Funding Purposes Shall Not Be Applied To Non-medicare Hospital Inpatient Days For Inpatient Psychiatric Services Provided By Nonstate Government Hospitals Licensed By The State Department Of Health To Operate Inpatient Psychiatric Beds; To Provide That If A Hospital Subject To The Assessment Closes Or Ceases To Operate Inpatient Specialty Beds Or Services That Are Recognized By The State Department Of Health As A Separate Licensure Category Of Beds, Then That Hospital's Assessment For The State Fiscal Year In Which The Specialty Beds Or Services Are Closed Or Cease To Operate Shall Be Adjusted On A Pro Rata Basis To Reflect The Number Of Days The Specialty Service Was Not In Service During The State Fiscal Year For Which The Assessment Is Being Assessed; To Extend The Date Of The Repealer On This Section; And For Related Purposes.</description><committee/><history><step><date>2013-01-21</date><action>Referred To Medicaid</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>1</importance></step><step><date>2013-02-05</date><action>Died In Committee</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>1</importance></step></history><sponsors><sponsor><people_id>6580</people_id><name>Scott DeLano</name><role_id>1</role_id><ftm_id>0</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>1</sponsor_order></sponsor><sponsor><people_id>13795</people_id><name>Timmy Ladner</name><role_id>1</role_id><ftm_id>0</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>2</sponsor_order></sponsor><sponsor><people_id>6515</people_id><name>Jessica Upshaw</name><role_id>1</role_id><ftm_id>5835</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>3</sponsor_order></sponsor><sponsor><people_id>6526</people_id><name>Richard Bennett</name><role_id>1</role_id><ftm_id>0</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>4</sponsor_order></sponsor><sponsor><people_id>13770</people_id><name>Greg Haney</name><role_id>1</role_id><ftm_id>0</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>5</sponsor_order></sponsor><sponsor><people_id>6643</people_id><name>David Baria</name><role_id>1</role_id><ftm_id>0</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>6</sponsor_order></sponsor><sponsor><people_id>13774</people_id><name>Sonya Williams-Barnes</name><role_id>1</role_id><ftm_id>0</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>7</sponsor_order></sponsor><sponsor><people_id>13776</people_id><name>Carolyn Crawford</name><role_id>1</role_id><ftm_id>0</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>8</sponsor_order></sponsor></sponsors><texts><text><doc_id>709292</doc_id><date>2013-01-28</date><type>Introduced</type><mime>text/html</mime><url>http://legiscan.com/MS/text/HB1184/id/709292</url><state_link>http://billstatus.ls.state.ms.us/documents/2013/html/HB/1100-1199/HB1184IN.htm</state_link></text></texts></bill></legiscan>
