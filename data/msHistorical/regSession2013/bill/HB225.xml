<legiscan><status></status><bill><bill_id>450956</bill_id><change_hash>a0224bc859969f24711381f80eea7c01</change_hash><session><session_id>1008</session_id><session_name>2013 Regular Session</session_name></session><url>http://legiscan.com/MS/bill/HB225/2013</url><state_link>http://billstatus.ls.state.ms.us/2013/pdf/history/HB/HB0225.xml</state_link><completed>0</completed><status>4</status><status_date>2013-03-14</status_date><progress><step><date>2013-01-14</date><event>1</event></step><step><date>2013-01-14</date><event>9</event></step><step><date>2013-01-22</date><event>2</event></step><step><date>2013-01-24</date><event>9</event></step><step><date>2013-03-12</date><event>3</event></step><step><date>2013-03-14</date><event>4</event></step></progress><state>MS</state><state_id>24</state_id><bill_number>HB225</bill_number><bill_type>B</bill_type><body>H</body><body_id>57</body_id><current_body>H</current_body><current_body_id>57</current_body_id><title>&quot;MS Hwy. Patrol Fallen Officer's Memorial Highway Act;&quot; enact to designate certain public roadways in honor of.</title><description>An Act To Create The &quot;mississippi Highway Patrol Fallen Officer's Memorial Highway Act&quot;; To Authorize The Mississippi Highway Safety Patrol Within The Department Of Public Safety, In Conjunction With The Mississippi Department Of Transportation To Designate Any Segment Of Any Public Street, Road, Highway Or Interstate Within The State Upon Which Any Member Of The Mississippi Highway Safety Patrol Has Fallen In The Line Of Duty As A Memorial Roadway; To Limit The Mileage Of The Segment Of Roadway To Be So Designated; To Require The Memorial Signage To Include A Distinctive Marker Designed By The Department Of Public Safety; To Provide For The Funding Of The Distinctive Markers To Be Placed On The Signage; To Provide That All Officers Of The Mississippi Highway Safety Patrol Who Have Fallen Since January 1, 1938 Shall Be Eligible For Such Memorial Roadway Designation; To Amend Section 65-3-38.1, Mississippi Code Of 1972, In Conformity To The Preceding Provisions; And For Related Purposes.</description><committee/><history><step><date>2013-01-14</date><action>Referred To Transportation</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>1</importance></step><step><date>2013-01-22</date><action>Title Suff Do Pass</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>0</importance></step><step><date>2013-01-22</date><action>Passed</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>1</importance></step><step><date>2013-01-24</date><action>Transmitted To Senate</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>0</importance></step><step><date>2013-01-24</date><action>Referred To Highways and Transportation</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>1</importance></step><step><date>2013-02-26</date><action>Title Suff Do Pass</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>0</importance></step><step><date>2013-03-07</date><action>Passed</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>0</importance></step><step><date>2013-03-08</date><action>Transmitted To House</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>0</importance></step><step><date>2013-03-12</date><action>Enrolled Bill Signed</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>1</importance></step><step><date>2013-03-12</date><action>Enrolled Bill Signed</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>0</importance></step><step><date>2013-03-14</date><action>Approved by Governor</action><chamber></chamber><chamber_id>0</chamber_id><importance>1</importance></step></history><sponsors><sponsor><people_id>6550</people_id><name>George Flaggs</name><role_id>1</role_id><ftm_id>413</ftm_id><sponsor_type_id>1</sponsor_type_id><sponsor_order>1</sponsor_order></sponsor><sponsor><people_id>6577</people_id><name>Earl