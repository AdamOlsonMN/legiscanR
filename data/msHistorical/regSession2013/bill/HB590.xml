<legiscan><status></status><bill><bill_id>463871</bill_id><change_hash>4ae04ffa31a359c6edf37a0059228893</change_hash><session><session_id>1008</session_id><session_name>2013 Regular Session</session_name></session><url>http://legiscan.com/MS/bill/HB590/2013</url><state_link>http://billstatus.ls.state.ms.us/2013/pdf/history/HB/HB0590.xml</state_link><completed>0</completed><status>6</status><status_date>2013-03-30</status_date><progress><step><date>2013-01-21</date><event>1</event></step><step><date>2013-01-21</date><event>9</event></step><step><date>2013-02-07</date><event>2</event></step><step><date>2013-02-12</date><event>9</event></step><step><date>2013-03-30</date><event>6</event></step></progress><state>MS</state><state_id>24</state_id><bill_number>HB590</bill_number><bill_type>B</bill_type><body>H</body><body_id>57</body_id><current_body>S</current_body><current_body_id>58</current_body_id><title>Mississippi Main Street Investment Act; create.</title><description>An Act To Create The Mississippi Main Street Investment Act; To Provide Definitions For This Act; To Create The Mississippi Main Street Investment Loan Fund; To Require Mda To Establish A Program To Make Loans To Municipalities To Assist With Maintaining And Improving The Viability Of Revitalization Zones; To Require A Municipality Desiring A Loan Under This Act To Submit An Application To Mda; To Provide That Mda Shall Review The Application Of A Municipality In Order To Confirm That The Revitalization Zone Meets The Requirements Of This Act; To Provide That The Mda Shall Establish A Deadline For The Submission Of Applications During A State Fiscal Year; To Provide That If A Municipality In Which A Revitalization Zone Is Located Receives A Loan Under This Section, Then The Sales Tax Revenue Collected Shall Be Allocated For Distribution To The Municipality In Which The Revitalization Zone Is Located And Paid To The Municipality; To Authorize Mda With All Powers Necessary To Implement And Administer The Program Authorized By This Act; To Amend Sections 27-65-75 And 27-67-31, Mississippi Code Of 1972, To Conform To The Provisions Of This Act; To Provide That The Proceeds Of Bonds Issued Pursuant To This Act Shall Be Deposited Into The Mississippi Main Street Investment Loan Fund; And For Related Purposes.</description><committee/><history><step><date>2013-01-21</date><action>Referred To Ways and Means</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>1</importance></step><step><date>2013-02-07</date><action>Title Suff Do Pass</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>0</importance></step><step><date>2013-02-07</date><action>Passed</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>1</importance></step><step><date>2013-02-08</date><action>Transmitted To Senate</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>0</importance></step><step><date>2013-02-12</date><action>Referred To Finance</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>1</importance></step><step><date>2013-03-14</date><action>Title Suff Do Pass As Amended</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>0</importance></step><step><date>2013-03-19</date><action>Amended</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>0</importance></step><step><date>2013-03-19</date><action>Passed As Amended</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>0</importance></step><step><date>2013-03-20</date><action>Returned For Concurrence</action><chamber>S</chamber><chamber_id>58</chamber_id><importance>0</importance></step><step><date>2013-03-21</date><action>Decline to Concur/Invite Conf</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>0</importance></step><step><date>2013-03-25</date><action>Conferees Named Smith (39th),Rogers (61st),Bennett</action><chamber>H</chamber><chamber_id>57</chamber_id><importance>0</importance></step><step><date>2013-03-27</d